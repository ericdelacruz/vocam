@{
    var contact = Session["PhoneNo"] as string;
    //var portalUrl = Session["PortalUrl"] as string;
    var portalUrl = "http://" + Request.Url.Host.Replace("www","portal");
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @if (ViewBag.Title != null)
    {
        <title>@ViewBag.Title</title>
    }
    else
    {
        <title>Safety On Demand</title>
    }

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="~/Content/css/colorbox/colorbox.css" rel="stylesheet" media="screen" />
    <link href="~/Content/css/main.css" rel="stylesheet" media="screen" />


    @if (ViewBag.Description != null)
    {
        <meta name="description" content="@ViewBag.Description" />
    }
    @if (ViewBag.Keywords != null)
    {
        <meta name="keywords" content="@ViewBag.Keywords" />
    }

</head>
<body>
    <!--[if lt IE 9]>
      <script type="text/javascript" src="js/respond.min.js"></script>
    <![endif]-->
    <header>
        <div class="container row">
            <div class="col-md-2"><a href="@Url.Action("index","home")"><img class="logo" src="~/Content/images/safetyOnDemand-logo.jpg" alt="Safety On Demand"></a></div>
            <div class="col-md-10">
                <div class="headerRight">
                @if(contact != null)
                {     
                    <div class="call"><span class="calltxt">CALL</span>@contact</div>
                }
                else
                {
                     <div class="call"><span class="calltxt">CALL</span> 03 9809 7777</div>
                }
                  <div class="socialBtns clearfix">

                        @*<ul>
                            <li><a class="socialTwitter" href="#">twitter</a></li>
                            <li><a class="socialYoutube" href="#">youtube</a></li>
                            <li><a class="socialFacebook" href="#">facebook</a></li>
                            <li><a class="socialLinkdin" href="#">linkdin</a></li>
                        </ul>*@
                    
                        <ul>
                            <li style="display:none;"><a class="socialTwitter" href="#">twitter</a></li>
                            <li style="display:none;"><a href="#" class="socialYoutube">youtube</a></li>
                            <li style="display:none;"><a href="#" class="socialFacebook">facebook</a></li>
                            <li style="display:none;"><a href="#" class="socialLinkdin">linkdin</a></li>
                        </ul>
                       <br />

                    </div>

                    <div class="navbar2 fr clearfix">
                        <ul class="menuNav">
                            <li id="activeHome">
                                @Html.ActionLink("HOME", "index", "home") @{
                                    if (ViewBag.CurPage == "Home")
                                    {
                                        <div class="arrowActive"></div>
                                        <style>
                                            #activeHome a { color: #F48518; }
                                        </style>
                                      
                                    }
                                }
                            </li>
                            <li id="activeCat">
                                @Html.ActionLink("CATEGORIES", "index", "categories")@{
                                    if (ViewBag.CurPage == "Category")
                                    {
                                        <div class="arrowActive"></div>
                                        <style>
                                            #activeCat a { color: #F48518; }
                                        </style>
                                    }
                                }
                            </li>
                            <li id="activeLearnMore">
                                @Html.ActionLink("LEARN MORE", "learnmore", "home")@{
                                    if (ViewBag.CurPage == "LearnMore")
                                    {
                                        <div class="arrowActive"></div>
                                        <style>
                                            #activeLearnMore a {
                                                color: #F48518;
                                            }
                                        </style>
                                    }
                                }
                            </li>
                            <li id="activeContact">
                                @Html.ActionLink("CONTACT", "contact", "home")@{
                                    if (ViewBag.CurPage == "Contact")
                                    {
                                        <div class="arrowActive"></div>
                                        <style>
                                            #activeContact a {
                                                color: #F48518;
                                            }
                                        </style>
                                    }
                                }
                            </li>

                            <li>
                                @*<button onclick="window.location = 'http://portal.sac-iis.com/'" type="submit" class="btn btn-sm btn-primary btn-block" style="padding:3px 8px; font-size:12px; font-weight:bold;">LOGIN</button>*@
                                <button onclick="window.location = '@portalUrl'" type="submit" class="btn btn-sm btn-primary btn-block" style="padding:3px 8px; font-size:12px; font-weight:bold;">LOGIN</button>
                            </li>

                        </ul>

                    </div>


                    <nav id="hideToggleNav" class="navbar navbar-default" role="navigation">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" style="float:left; margin-top:0px !important;">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <div class="call2 clearfix"><span class="calltxt">CALL</span> 03 9809 7777</div>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1" style="height:auto !important;">


                            <ul id="sodaMenu" class="nav navbar-nav navbar-left fl">
                                <li>@Html.ActionLink("HOME", "index", "home")</li>
                                <li>@Html.ActionLink("CATEGORIES", "index", "categories")</li>
                                <li>@Html.ActionLink("LEARN MORE", "learnmore", "home")</li>
                                <li>@Html.ActionLink("CONTACT", "contact", "home")</li>
                                <li><a onclick ="window.location = '@portalUrl'">LOGIN</a></li>
                            </ul>


                        </div><!-- /.navbar-collapse -->
                    </nav>

                    @*<div class="arrowActive arrowHome"></div>*@
                </div> <!-- end of headerRight-->
            </div>
        </div>
    </header>

    <div>
        @RenderBody()
    </div>

    <footer>
        <div class="container row">
            <div class="col-md-2"><a href="@Url.Action("index","home")"><img src="~/Content/images/safetyOnDemand-logoFooter.jpg" alt="Safety On Demand"></a></div>
            <div align="right" class="col-md-10">
                <div class="ftrLinks whitespace">
                    <div class="clearfix">
                       <ul class="footNav">
                            <li>@Html.ActionLink("HOME", "index", "home")</li>
                            <li>@Html.ActionLink("CATEGORIES", "index", "categories")</li>
                            <li>@Html.ActionLink("LEARN MORE", "learnmore", "home")</li>
                            <li>@Html.ActionLink("CONTACT", "contact", "home")</li>
                            <li>@Html.ActionLink("SITEMAP", "sitemap", "home")</li>
                            <li>@Html.ActionLink("LEGALS", "legals", "home")</li>
                        </ul>
                        
                    </div>
                    <div id="copyrightTxt" class="clearfix"><span>Copyright &copy; @DateTime.Now.Year</span></div>
                </div>
            </div>

        </div>
    </footer>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="/Scripts/jquery.min.js"></script>
    <script src="/Scripts/jquery.colorbox.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="/Scripts/bootstrap.min.js"></script>
    <script src="/Scripts/bootstrap-dropdown.js"></script> 
   
    
    <script src="/Scripts/jquery.prettyPhoto.js" type="text/javascript"></script>
    <script type="text/javascript">
        //$(document).ready(function () {
          //  $("a[rel^='freePowerPoint']").prettyPhoto();
            //$("#recaptcha_reload").hide();
          
        //});
        $("#iframe").colorbox({ iframe: true, width: 550, height: 600, innerWidth: 550, innerHeight: 550 });
    </script>

    <script src="/Scripts/jquery.form.min.js"></script>
    <script src="/Scripts/my-form-script.js"></script>

</body>
</html>
